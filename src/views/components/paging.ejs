<% if (pageCount > 1) { %>
<nav aria-label="Page navigation">
  <ul class="pagination">
    <% for ( var i = 1; i <= pageCount; i++ ) { %>
    <li class="<%= page == i ? 'active' : '' %>">
      <a href="<%= next %>?p=<%= i %>#comments-container">
        <%= i %>
      </a>
    </li>
    <% } %>
  </ul>
</nav>
<% } %>
